{
  "files.exclude": {
    "**/.git": false,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/build": false,
    "**/buildAndroid": true,
    "**/tmp": true,
    "**/.webpack": true,
    "**/.bundle": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/tmp": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/tmp": true,
    "**/*.bundle": true,
    "**/*.map": true,
    "**/.webpack": true,
    "**/target": true
  },
  "extensions.autoUpdate": true,
  "[objective-c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[objective-cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[swift]": {
    "editor.defaultFormatter": "vknabel.vscode-apple-swift-format"
  },
  "[kotlin]": {
    "editor.defaultFormatter": "esafirm.kotlin-formatter"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.rulers": [80],
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "Tokyo Night",
  "window.autoDetectColorScheme": true,
  "workbench.layoutControl.enabled": false,
  "workbench.welcomePage.walkthroughs.openOnInstall": false,
  "editor.formatOnSave": true,
  "editor.fontFamily": "'JetBrains Mono', Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "comments": "#ff5c11",
    // "comments": "#54ff51",
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  },
  "workbench.settings.editor": "json",
  "telemetry.telemetryLevel": "off",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "git.openRepositoryInParentFolders": "never",
  "peacock.affectTitleBar": false,
  "peacock.affectActivityBar": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.scrollback": 30000,
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true
  },
  "workbench.enableExperiments": false,
  "workbench.list.smoothScrolling": true,
  "workbench.fontAliasing": "auto",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "cmake.showOptionsMovedNotification": false,
  "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "workbench.tree.indent": 18,
  "rust-analyzer.files.excludeDirs": [
    "opacity-ios",
    "react-native-opacity",
    "opacity-android",
    "android",
    "scripts",
    "generated"
  ],
  "rust-analyzer.cargo.target": "aarch64-apple-ios",
  "rust-analyzer.cargo.features": "all",
  "cSpell.userWords": [
    "aarch",
    "androideabi",
    "appcast",
    "ARCHS",
    "armeabi",
    "armv",
    "autogen",
    "bridgeless",
    "cbindgen",
    "cctx",
    "clippy",
    "clsx",
    "cocoapod",
    "cocoapods",
    "codegen",
    "codibly",
    "compat",
    "dylib",
    "ETAG",
    "fa√ßade",
    "fqdb",
    "hasher",
    "immer",
    "jstring",
    "libc",
    "libmy",
    "libsql",
    "libsqlite",
    "lipo",
    "luxon",
    "nativemodule",
    "nativewind",
    "oslog",
    "ospfranco",
    "podspec",
    "println",
    "reqwest",
    "rowid",
    "runtimes",
    "rustc",
    "rustup",
    "samplesdk",
    "sqlx",
    "staticlib",
    "tailwindcss",
    "tauri",
    "tccutil",
    "thiserror",
    "tlsn",
    "trnslcd",
    "typecheck",
    "typeorm",
    "XCFRAMEWORK",
    "xcodebuild",
    "xxxs"
  ],
  "git.autofetch": true,
  "makefile.configureOnOpen": false,
  "diffEditor.experimental.showMoves": true,
  "extensions.ignoreRecommendations": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.minimap.enabled": false,
  "vim.smartRelativeLine": true,
  "githubPullRequests.pullBranch": "never",
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "window.confirmSaveUntitledWorkspace": false
}
